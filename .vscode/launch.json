{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Launch (console with args)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "dotnet build upfgconsole",
            "program": "${workspaceFolder}/upfgconsole/bin/Debug/net8.0/upfgconsole.dll",
            "args": ["missions/shuttle.json"],
            "cwd": "${workspaceFolder}/upfgconsole",
            "console": "integratedTerminal",
            "stopAtEntry": false
        }
    ]
}